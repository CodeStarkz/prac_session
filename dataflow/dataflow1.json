{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql",
						"type": "DatasetReference"
					},
					"name": "sourcedbsql"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "filter1"
				},
				{
					"name": "sort1"
				},
				{
					"name": "filter2"
				},
				{
					"name": "sort2"
				},
				{
					"name": "union1"
				},
				{
					"name": "TotalSale"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Branch_ID as string,",
				"          Dealter_ID as string,",
				"          Model_ID as string,",
				"          Revenue as integer,",
				"          Units_Sold as integer,",
				"          Date_ID as string,",
				"          Day_ as integer,",
				"          Month_ as integer,",
				"          Year_ as integer,",
				"          BranchName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> sourcedbsql",
				"sourcedbsql filter(Model_ID == \"BMW-M1\" ) ~> filter1",
				"filter1 sort(desc(Units_Sold, true)) ~> sort1",
				"sourcedbsql filter(Model_ID== \"Mer-M8\") ~> filter2",
				"filter2 sort(desc(Units_Sold, true)) ~> sort2",
				"sort2, sort1 union(byName: true)~> union1",
				"union1 aggregate(groupBy(Model_ID),",
				"     Units_Sold = sum(Units_Sold)) ~> TotalSale"
			]
		}
	}
}